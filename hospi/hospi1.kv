<ZeroWindow>:
    name:'zero'

    BoxLayout:
        orientation:'vertical'
        size:root.width,root.height

        MDToolbar:
            title: 'Welcome'

            MDLabel:
                text:str(root.t)


        RelativeLayout:
            size:300,300
            Button:

                background_color:0,0,0,0
                mode:"rectangle"
                size_hint:.17,.14

                pos_hint:{'center_x':.6,'center_y':.7}
                on_press:root.manager.current = "seven"
                Image:
                    source:"m6.png"
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
                    size:self.parent.size
                    stretch:True
